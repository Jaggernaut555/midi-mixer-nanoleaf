{
  "$schema": "./node_modules/midi-mixer-plugin/plugin.schema.json",
  "version": "0.0.7",
  "id": "ca.jaggernaut.midi-mixer.nanoleaf",
  "name": "Nanoleaf",
  "type": "node",
  "main": "lib/index.js",
  "icon": "icon.png",
  "settings": {
    "status": {
      "label": "Panel 1 Connection Status",
      "type": "status",
      "fallback": "Not connected"
    },
    "ip": {
      "label": "IP Address",
      "required": true,
      "type": "text",
      "fallback": "ip address"
    },
    "port": {
      "label": "Port",
      "type": "text",
      "fallback": "16021"
    },
    "apikey": {
      "label": "API Key",
      "required": true,
      "type": "password",
      "fallback": "No API Key"
    },
    "status2": {
      "label": "Panel 2 Connection Status",
      "type": "status",
      "fallback": "Not connected"
    },
    "ip2": {
      "label": "IP Address 2",
      "required": true,
      "type": "text",
      "fallback": "ip address"
    },
    "port2": {
      "label": "Port 2",
      "type": "text",
      "fallback": "16021"
    },
    "apikey2": {
      "label": "API Key 2",
      "required": true,
      "type": "password",
      "fallback": "No API Key"
    },
    "syncinterval": {
      "label": "Sync interval (seconds)",
      "required": false,
      "type": "text",
      "fallback": "15"
    },
    "iprequest": {
      "label": "IP to request apikey. Enable pairing mode on the device before pressing request.",
      "type": "text",
      "fallback": "ip address"
    },
    "tokenrequest": {
      "label": "Request apikey",
      "type": "button"
    },
    "tokenstatus": {
      "label": "Requested API key",
      "type": "status",
      "fallback": "no key"
    },
    "ipsearchstatus": {
      "label": "Discovered nanoleaf panels",
      "type": "status",
      "fallback": "Not searched"
    },
    "ipsearchbutton": {
      "label": "Search network for panels",
      "type": "button"
    }
  },
  "author": "Jaggernaut"
}